<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.evtukhov.android.wishlist">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_note_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".CreateNoteActivity"></activity>
        <activity android:name=".NoteListActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".EnterPinActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>